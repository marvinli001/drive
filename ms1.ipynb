# check python basic
class Student:
    stuCount = 0
    def __init__(self, name):
        self.name = name
        self.grade = {}
        Student.stuCount += 1
    
    def add_grade(self, name, grade):
        self.grade[name] = grade
    
    def display_count(self):
        print('Total Student Count %d' % Student.stuCount)
    
    def display_student(self):
        print('Name:', self.name, ', Average Grade:', self.get_ave_grade())

    def get_ave_grade(self):
        total_score = 0
        for class_name in self.grade:
            total_score += self.grade[class_name]
        return total_score/len(self.grade)

student_a = Student('David')

student_a.add_grade('math', 81)
student_a.add_grade('biology', 85)
student_a.add_grade('biology', 60)
student_a.add_grade('english', 93)
student_a.display_student()

